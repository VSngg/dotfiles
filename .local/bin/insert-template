#!/bin/sh

_GO=$(
cat << EOF 
package main

import (
    "fmt"
)

func main() {

}
EOF
)

_SH=$(
cat << EOF
#!/bin/sh

main() {

}
EOF
)

if test "$1" = "go"; then
    echo "$_GO"
fi
if test "$1" = "sh"; then
    echo "$_SH"
fi
if test "$#" -lt 1; then
    echo "usage: insert-template <go,sh>" >&2
    exit
fi

